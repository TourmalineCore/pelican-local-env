# Pelican local env, AGPL-3.0 license

replicaCount: 2
updateStrategy:
  rollingUpdate:
      maxUnavailable: 1
      maxSurge: 1

readinessProbe:
  enabled: true
  path: /
  initialDelaySeconds: 15
  periodSeconds: 30

livenessProbe:
  enabled: true
  path: /
  initialDelaySeconds: 45
  periodSeconds: 60

extraConfigMapEnvVars:
  API_URL: "http://pelican-cms-nginx:40110/api"
  PREVIEW_SECRET: "secret"